<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Proyecto_Examen_Complexivo" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/gris_oscuro</item>
        <item name="colorPrimaryDark">@color/gris</item>
        <item name="colorAccent">@color/teal_200</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Proyecto_Examen_Complexivo.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Proyecto_Examen_Complexivo.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Proyecto_Examen_Complexivo.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>