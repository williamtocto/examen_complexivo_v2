<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".ServicioDetalle"
    tools:layout_editor_absoluteY="25dp">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="69dp"
        android:layout_height="75dp"
        android:layout_marginBottom="172dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/compras" />

    

    <ImageView
        android:id="@+id/imgfotoproductocompra3"
        android:layout_width="232dp"
        android:layout_height="166dp"
        android:layout_marginTop="32dp"
        android:contentDescription="@string/todosss"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ImageContrastCheck"

        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/txtPrecio_Producto3"
        android:layout_width="297dp"
        android:layout_height="41dp"
        android:layout_marginTop="212dp"
        android:text="@string/textviewasd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtNombre_Producto3"
        android:layout_width="292dp"
        android:layout_height="71dp"
        android:layout_marginTop="316dp"
        android:text="@string/textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="180dp"
        android:layout_height="28dp"
        android:layout_marginTop="388dp"
        android:text="@string/cantidaddeee"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.242"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextCantidadCarrito"
        android:layout_width="289dp"
        android:layout_height="49dp"
        android:layout_marginTop="428dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.459"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/txtDescripcionProducto3"
        android:layout_width="306dp"
        android:layout_height="39dp"
        android:layout_marginTop="264dp"
        android:text="@string/textviewasdasd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
            android:id="@+id/btnGuardarCarrito"
            android:layout_width="291dp"
            android:layout_height="58dp"
            android:layout_marginTop="588dp"
            android:text="@string/qqqq"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.758"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Empresa"
            android:layout_width="185dp"
            android:layout_height="25dp" android:id="@+id/txt_empresa"
            app:layout_constraintTop_toBottomOf="@+id/editTextCantidadCarrito"
            android:layout_marginTop="56dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="68dp" app:layout_constraintBottom_toTopOf="@+id/btnGuardarCarrito"
            app:layout_constraintVertical_bias="0.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>